<?xml version="1.0" encoding="utf-8"?>
<!--
  empty web.config file for octopus deployment.
  + correct mime type for woff (IIS)
  -->
  <configuration>
      <system.webServer>
          <staticContent>
              <mimeMap fileExtension="woff" mimeType="application/font-woff" />
              <mimeMap fileExtension="woff2" mimeType="application/font-woff" />
          </staticContent>
      </system.webServer>
  </configuration>
